

















































































































































































































































































#include <bits/stdc++.h>
using namespace std;
class person{
    public:
    string name;
    int age;
};
class student: public person{
  public:
  int rollno;
};
class teacher: public person{
    public:
    string subject;
};

int main() {
    teacher t1;
    t1.name="shyam";
    t1.subject="flutter";
    t1.age=15;
    
    cout<<t1.name<<t1.subject<<t1.age<<endl;
    
    student s1;
    s1.name="krishna";
    s1.age=21;
    s1.rollno=2731;
    cout<<s1.name<<s1.rollno<<s1.age<<endl;
    
	return 0;
}
